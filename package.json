{
  "name": "react-parcel-lint",
  "version": "1.0.0",
  "author": "ashwin bordoloi",
  "main": "src/index.js",
  "license": "MIT",
  "parcel-plugin-structurize": false,
  "scripts": {
    "clean": "rimraf ./dist .cache",
    "start": "cross-env NODE_ENV=development parcel ./public/index.html --no-autoinstall --open",
    "build": "yarn clean && cross-env NODE_ENV=production parcel build ./public/index.html --no-cache --no-autoinstall",
    "deploy": "yarn build && firebase deploy -P staging --only hosting:bizbuzzzv2-dev-customer",
    "deploy-prod": "yarn build && firebase deploy -P qatst --only hosting:bizbuzzzv2-qa-customer",
    "sourcemap": "source-map-explorer --html ./dist/index.js ./dist/index.map > source-map.html",
    "generate": "plop"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ashwin1014/react-parcel-starter.git"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ]
  },
  "precommit": "lint-staged",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.8.3",
    "@babel/plugin-transform-react-inline-elements": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "add": "^2.0.6",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-parcel": "^1.10.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "global": "^4.4.0",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-compress": "^1.1.0",
    "parcel-plugin-imagemin": "^3.0.0",
    "parcel-plugin-structurize": "^1.1.6",
    "parcel-plugin-svg-sprite": "^1.4.1",
    "plop": "^2.5.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "querystring": "^0.2.0",
    "remove": "^0.1.5",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-localization": "^1.0.15",
    "react-router-dom": "^5.1.2"
  }
}
